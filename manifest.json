{
	"name": "PeopleSoft Financial Data Improvement Program",
	"version": "1.0",
	"description": "A web crawler for PeopleSoft Financial data",
	"permissions": [
		"tabs",
		"activeTab",
		"storage",
		"declarativeContent"
	],
	"background":{
		"scripts": [
			"scripts/background.js"
		],
		"persistent": false
	},
	"browser_action": {
      	"default_popup": "html/popup.html"
    },
	"manifest_version": 2
}